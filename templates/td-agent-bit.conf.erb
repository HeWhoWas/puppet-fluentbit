# Managed by puppet
# td agent bit config file:
# configure service and include all other config files
[SERVICE]
    <% if @flush -%>
    Flush           <%= @flush %>
    <% end -%>
    <% if @daemon == 'on'-%>
    Daemon          <%= @daemon %>
    <% end -%>
    <% if @log_file -%>
    Log_File        <%= @log_file %>
    <% end -%>
    <% if @log_level -%>
    Log_Level       <%= @log_level %>
    <% end -%>
    <% if @parsers_file -%>
    Parsers_File    <%= @parsers_file %>
    <% end -%>
    <% if @plugins_file -%>
    Plugin_File     <%= @plugins_file %>
    <% end -%>
    <% if @streams_file -%>
    Streams_file    <%= @streams_file %>
    <% end -%>
    <% if @http_server == 'on' -%>
    HTTP_Server     <%= @http_server %>
    HTTP_Listen     <%= @http_listen %>
    HTTP_Port       <%= @http_port %>
    <% end -%>
    <% if @coro_stack_size -%>
    Coro_Stack_Size <%= @coro_stack_size %>
    <% end -%>

@INCLUDE inputs_*.conf
@INCLUDE outputs_*.conf
